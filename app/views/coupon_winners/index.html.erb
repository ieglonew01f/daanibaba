<p id="notice"><%= notice %></p>

<h1>Listing Coupon Winners</h1>

<table>
  <thead>
    <tr>
      <th>Coupon</th>
      <th>Ip</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @coupon_winners.each do |coupon_winner| %>
      <tr>
        <td><%= coupon_winner.coupon_id %></td>
        <td><%= coupon_winner.ip %></td>
        <td><%= link_to 'Show', coupon_winner %></td>
        <td><%= link_to 'Edit', edit_coupon_winner_path(coupon_winner) %></td>
        <td><%= link_to 'Destroy', coupon_winner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Coupon winner', new_coupon_winner_path %>
